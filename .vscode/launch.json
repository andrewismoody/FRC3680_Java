  "version": "0.2.0",
  "configurations": [
    {
      "type": "wpilib",
      "name": "WPILib Desktop Debug",
      "request": "launch",
      "desktop": true,
    },
    {
      "type": "wpilib",
      "name": "WPILib roboRIO Debug",
      "request": "launch",
      "desktop": false,
    },
    {
      "name": "Run AutoJsonBuilder (WPF)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build AutoJsonBuilder",
      "program": "${workspaceFolder}/tools/AutoJsonBuilder/bin/Debug/net8.0-windows/AutoJsonBuilder.exe",
      "cwd": "${workspaceFolder}/tools/AutoJsonBuilder",
      "console": "integratedTerminal",
      "stopAtEntry": false
    }
  ]
}
